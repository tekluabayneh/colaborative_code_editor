  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["cypress"]
  },
  "include": [
    "E2E/cypress/e2e/**/*.ts",
    "E2E/cypress/e2e/**/*.tsx"
  ]
}

